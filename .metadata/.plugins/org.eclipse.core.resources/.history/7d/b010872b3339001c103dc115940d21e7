#include <stdio.h>
#include <stdlib.h>

int main(void) {

	 int i;
	 printf("!!!Hello World!!!");

	  for (i = 1; i < 11; ++i)
	  {
	    printf("%d ", i);
	  }
	  return 0;/* prints !!!Hello World!!! */
	return EXIT_SUCCESS;

}
