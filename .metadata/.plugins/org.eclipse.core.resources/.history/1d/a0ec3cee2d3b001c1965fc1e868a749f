#include <stdio.h>
#include <stdlib.h>

int main(void) {
	int i,j,k,m;
	for(i=1; i<10; i++){
		if(i==1){
			for(j=1; j<3; j++){
				printf("* ");
			}
		}else if(i==4){
			for(k=1; k<7; k++){
				printf("* ");
			}
		}else{
			printf("* ");
		}
		printf("\n");
	}
	return EXIT_SUCCESS;
}
