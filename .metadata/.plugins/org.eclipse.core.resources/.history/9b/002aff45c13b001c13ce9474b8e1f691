#include <stdio.h>
#include <stdlib.h>

int main(void) {
	int a[10],b[10],i,j,k,temp[10],limit;
	printf("enter the limit \n");
	scanf("%d",&limit);
	printf("enter the first array elements\n");
	for(i=0; i<limit; j=i++){
		scanf("%d",&a[i]);

	}
	printf("enter the second array elements\n");
	for(j=1; j<limit; j++){
		scanf("%d",b[j]);
	}
	temp[k]=a[i];
	a[i]=b[j];
	b[j]=temp[k];
	for(i=0; i<limit; i++){
		printf("%d",a[i]);
	}
	for(j=0; j<limit; j++){
			printf("%d",b[j]);
		}

	return EXIT_SUCCESS;
}
